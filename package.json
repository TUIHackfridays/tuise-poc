{
  "name": "react-redux-donderstarter",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "npm": ">=4"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start": "npm run preinstall && npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color; exit 0",
    "lint:watch": "esw webpack.config.* src tools --color --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter nyan",
    "test:cover": "nyc npm test",
    "test:watch": "npm run test -- --watch --color | grep -v '^    at .*/node_modules/'"
  },
  "author": "",
  "license": "MIT",
